{
  "name": "leaderlink",
  "version": "1.0.0",
  "private": true,
  "author": "Robert Dalla & Tom Banh",
  "description": "Mobile application for PowerLink",
  "homepage": "https://github.com/robertdalla/LeaderLink",
  "repository": {
    "type": "git",
    "url": "https://github.com/robertdalla/LeaderLink.git"
  },
  "engines": {
    "node": "15 || 16",
    "npm": ">=6"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:mock": "concurrently --kill-others \"npm run mock:server\" \"npm run start\"",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:mock": "concurrently --kill-others \"npm run mock:server:e2e\" \"npm run e2e\"",
    "build:prod": "ng build --prod --build-optimizer=true",
    "mock:server": "nodemon --watch mocks --exec json-server mocks/data.js --routes mocks/routes.json",
    "mock:server:e2e": "nodemon --watch mocks --exec json-server mocks/data-e2e.js --routes mocks/routes.json"
  },
  "dependencies": {
    "@angular/animations": "^12.2.17",
    "@angular/common": "^12.2.17",
    "@angular/core": "^12.2.17",
    "@angular/forms": "^12.2.17",
    "@angular/localize": "^12.2.17",
    "@angular/platform-browser": "^12.2.17",
    "@angular/platform-browser-dynamic": "^12.2.17",
    "@angular/router": "^12.2.17",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "@ngrx/router-store": "^12.5.1",
    "@ngrx/store": "^12.5.1",
    "@ngrx/store-devtools": "^12.5.1",
    "angular2-draggable": "^2.3.2",
    "bootstrap": "^5.2.3",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.27.2",
    "font-awesome": "^4.7.0",
    "invariant": "^2.2.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "ngrx-store-localstorage": "^12.0.1",
    "ngx-spinner": "^12.0.0",
    "rxjs": "^6.6.7",
    "stacktrace-js": "^2.0.2",
    "tslib": "^2.0.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.18",
    "@angular/cli": "^12.2.18",
    "@angular/compiler": "^12.2.17",
    "@angular/compiler-cli": "^12.2.17",
    "@angular/language-service": "^12.2.17",
    "@angularclass/hmr": "^3.0.0",
    "@types/jasmine": "^4.3.1",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "~16.18.12",
    "codelyzer": "^6.0.2",
    "concurrently": "^7.6.0",
    "faker": "~5.5.3",
    "jasmine-core": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "json-server": "^0.17.1",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "nodemon": "^2.0.20",
    "protractor": "^7.0.0",
    "ts-node": "^10.9.1",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}
